{
  "lsp": {
    "rust-analyzer": {
      "initialization_options": {
        "cargo": {
          "features": "all"
        },
        "check": {
          "command": "clippy",
          "extraArgs": [
            "--locked",
            "--no-deps",
            "--target-dir=target/rust-analyzer"
          ]
        }
      }
    }
  }
}
